{
    "name": "projectsaturnstudios/laravel-x-fer",
    "description": "Transfer files between servers using SFTP or AWS S3",
    "type": "library",
    "version": "0.1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Project Saturn Studios, LLC",
            "email": "angel@projectsaturnstudios.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/events": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "spatie/laravel-data": "^4.0",
        "league/flysystem-sftp-v3": "^3.28",
        "league/flysystem-aws-s3-v3": "^3.28"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-laravel": "*",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "ProjectSaturnStudios\\Xfer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProjectSaturnStudios\\Xfer\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "ProjectSaturnStudios\\Xfer\\Providers\\XferServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test-clover": "vendor/bin/phpunit --coverage-clover coverage.xml",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "keywords": [
        "laravel",
        "file-transfer",
        "sftp",
        "s3",
        "filesystem",
        "xfer"
    ]
}
